        app:layout_constraintStart_toEndOf="@id/vg"
        app:layout_constraintTop_toBottomOf="@+id/toadyLeaveDiv"
        tools:targetApi="m" />

    <View
        android:id="@+id/monthStaffDiv"
        android:layout_width="0dp"
        android:layout_height="@dimen/div_h"
        android:background="@color/black6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@id/vg"
        app:layout_constraintTop_toBottomOf="@+id/monthStaffLeave" />

    <ImageView
        android:id="@+id/monthLeaveSummaryIc"
        android:layout_width="@dimen/enterMarksIc_w"
        android:layout_height="@dimen/enterMarksIc_w"
        android:layout_marginTop="@dimen/mTop1"
        android:foreground="?android:attr/selectableItemBackground"
        android:src="@drawable/staff3"
        app:layout_constraintEnd_toStartOf="@id/vg"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/monthStaffDiv"
        android:contentDescription="@string/default_cont_desc"
        tools:targetApi="m" />

    <TextView
        android:id="@+id/monthLeaveSummary"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:foreground="?android:attr/selectableItemBackground"
        android:padding="@dimen/pTop1"
        android:text="Staff Month Wise Leave Summary"
        android:textColor="@color/black"
        android:textSize="@dimen/md_size_text"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/vg"
        app:layout_constraintTop_toBottomOf="@+id/monthStaffDiv"
        tools:targetApi="m" />

    <View
        android:id="@+id/monthLeaveSummaryDiv"
        android:layout_width="0dp"
        android:layout_height="@dimen/div_h"
        android:background="@color/black6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@id/vg"
        app:layout_constraintTop_toBottomOf="@+id/monthLeaveSummary" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/vg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent=".22" />


