<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/bodyContainer2"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:padding="@dimen/pTop0"
    android:background="@color/white">


    <ImageView
        android:id="@+id/staffLeaveTodayIc"
        android:layout_width="@dimen/enterMarksIc_w"
        android:layout_height="@dimen/enterMarksIc_w"
        android:layout_marginTop="@dimen/mTop1"
        android:foreground="?android:attr/selectableItemBackground"
        android:src="@drawable/staff3"
        app:layout_constraintEnd_toStartOf="@id/vg"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:contentDescription="@string/default_cont_desc"
        tools:targetApi="m" />

    <TextView
        android:id="@+id/staffLeaveToday"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:foreground="?android:attr/selectableItemBackground"
        android:padding="@dimen/pTop1"
        android:text="Staff On Leave Today"
        android:textColor="@color/black"
        android:textSize="@dimen/md_size_text"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/vg"
        app:layout_constraintTop_toTopOf="parent"
        tools:targetApi="m" />

    <View
        android:id="@+id/toadyLeaveDiv"
        android:layout_width="0dp"
        android:layout_height="@dimen/div_h"
        android:background="@color/black6"
